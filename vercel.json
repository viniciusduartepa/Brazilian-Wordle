{
    "version": 2,
    "builds": [
      {
        "src": "build/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/build/static/$1"
        },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }